                  "type": "boolean"
                },
                "content": {
                  "type": "string",
                  "description": "The new content to write to the document"
                },
                "documentId": {
                  "type": "string",
                  "description": "The Quip document URL or ID to edit"
                },
                "format": {
                  "type": "string",
                  "enum": [
                    "html",
                    "markdown"
                  ],
                  "default": "markdown",
